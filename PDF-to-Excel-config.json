{
  "app": {
    "name": "PDF to Excel Converter",
    "version": "1.0.0",
    "domain": "www.yourmultitools.space"
  },
  "features": {
    "maxFileSize": 50,
    "supportedFormats": ["xlsx", "csv", "ods"],
    "tableDetection": ["auto", "manual"],
    "options": {
      "preserveFormatting": true,
      "extractImages": false,
      "skipEmptyCells": true
    }
  },
  "ui": {
    "themes": ["dark", "light"],
    "defaultTheme": "dark",
    "animations": {
      "particles": true,
      "floating": true
    }
  },
  "conversion": {
    "quality": "high",
    "retryAttempts": 3,
    "timeout": 300000
  },
  "privacy": {
    "localProcessing": true,
    "dataRetention": "none",
    "cookies": ["theme_preference"]
  }
}
